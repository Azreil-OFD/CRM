{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "db:up": {
      "cache": false
    },
    "db:down": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "db:up"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist/**",
        "{projectRoot}/.nuxt/**"
      ],
      "cache": true
    }
  }
}